facility
str(facility)

facility<-facility%>%
  mutate(총_집객시설수=집객시설_수+관공서_수+은행_수+종합병원_수+일반_병원_수+약국_수+유치원_수+초등학교_수+중학교_수+
                  고등학교_수+대학교_수+백화점_수+슈퍼마켓_수+극장_수+숙박_시설_수+
                  공항_수+철도_역_수+버스_터미널_수+지하철_역_수+버스_정거장_수)
is.na(facility)
facility[is.na(facility)]<-0

