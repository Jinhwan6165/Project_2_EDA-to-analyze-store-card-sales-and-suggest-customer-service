  mutate(확진일, 월=month(확진일))

covid_19<-covid_19%>%
 mutate(월, 분기=ifelse(월%in%c(1,2,3),1,ifelse(월%in%c(4,5,6),2,ifelse(월%in%c(7,8,9),3,4))))

#is.factor(covid_19$분기)

